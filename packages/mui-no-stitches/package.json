{
  "name": "@mui/no-stitches",
  "version": "0.0.1-alpha.6",
  "private": false,
  "description": "Processors to be used with Linaria for zero runtime CSS builds using stitches at build-time.",
  "keywords": [
    "react",
    "react-component",
    "zero-runtime"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:legacy && yarn build:modern && yarn build:node && yarn build:stable && yarn build:types && yarn build:copy-files",
    "build:legacy": "node ../../scripts/build.mjs legacy",
    "build:modern": "node ../../scripts/build.mjs modern",
    "build:node": "node ../../scripts/build.mjs node",
    "build:stable": "node ../../scripts/build.mjs stable",
    "build:copy-files": "node ../../scripts/copyFiles.mjs",
    "build:types": "node ../../scripts/buildTypes.mjs",
    "prebuild": "rimraf build tsconfig.build.tsbuildinfo",
    "release": "yarn build && npm publish build",
    "test": "cd ../../ && cross-env NODE_ENV=test mocha 'packages/mui-system/**/*.test.{js,ts,tsx}'",
    "typescript": "tslint -p tsconfig.json \"{src,test}/**/*.{spec,d}.{ts,tsx}\" && tsc -p tsconfig.json",
    "typescript:module-augmentation": "node scripts/testModuleAugmentation.js"
  },
  "peerDependencies": {
    "react": "^16.3.0 || ^17.0.0 || ^18.0.0",
    "@linaria/vite": "^4.5.4",
    "next-with-linaria": "^0.5.1"
  },
  "peerDependenciesMeta": {},
  "dependencies": {
    "@babel/types": "^7.22.5",
    "@linaria/babel-preset": "^4.5.4",
    "@linaria/tags": "^4.5.4",
    "@linaria/utils": "^4.5.3",
    "@stitches/react": "^1.2.8"
  },
  "devDependencies": {
    "@stitches/react": "^1.2.8",
    "@linaria/vite": "^4.5.4",
    "next-with-linaria": "^0.5.1",
    "vite": "^4.4.8"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "module": "./index.js",
  "types": "./index.d.ts",
  "linaria": {
    "tags": {
      "styled": "./styled.js",
      "css": "./css.js",
      "globalCss": "./globalCss.js",
      "icss": "./icss.js",
      "keyframes": "./keyframes.js"
    }
  }
}
